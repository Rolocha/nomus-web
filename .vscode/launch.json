{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "[client] Jest All",
      "program": "${workspaceFolder}/client/node_modules/.bin/jest",
      "args": [
        "--runInBand",
        "--config",
        "${workspaceFolder}/client/jest.config.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "cwd": "${workspaceFolder}/client",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "[client] Jest Current File",
      "program": "${workspaceFolder}/client/node_modules/.bin/jest",
      "args": [
        "${fileBasenameNoExtension}",
        "--config",
        "${workspaceFolder}/client/jest.config.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "cwd": "${workspaceFolder}/client",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "[server] Jest All",
      "program": "${workspaceFolder}/client/node_modules/.bin/jest",
      "args": [
        "--runInBand",
        "--config",
        "${workspaceFolder}/server/package.json"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "cwd": "${workspaceFolder}/server",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/server/node_modules/**/*"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "[server] Jest Current File",
      "program": "${workspaceFolder}/server/node_modules/.bin/jest",
      "args": [
        "${relativeFile}",
        "--config",
        "${workspaceFolder}/server/package.json"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "cwd": "${workspaceFolder}/server",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/server/node_modules/**/*"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "[server] Attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "localRoot": "${workspaceFolder}/server",
      "remoteRoot": "/server",
      "sourceMaps": true,
      "trace": true,
      "protocol": "inspector",
      "sourceMapPathOverrides": {
        "/*": "${workspaceRoot}/*"
      }
    }
  ]
}
