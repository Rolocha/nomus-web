    "workbox-webpack-plugin": "4.3.1",
    "yup": "^0.30.0",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "dev": "yarn && NODE_ENV=development webpack serve --config config/webpack.dev.js --host 0.0.0.0 --open --useLocalIp --hot",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "test": "node scripts/test.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:ci": "eslint --ext .js,.jsx,.ts,.tsx src --max-warnings 0",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "@emotion"
    ],
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "always"
        }
      ],
      "@emotion/pkg-renaming": "error",
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts",
      "jest-extended"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/test-utils"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.9.0",
    "@babel/preset-typescript": "^7.12.16",
    "@emotion/babel-plugin": "^11.1.2",
    "@emotion/eslint-plugin": "^11.0.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-system": "^5.1.9",
    "@types/yup": "^0.29.9",
    "@webpack-cli/serve": "^1.2.2",
    "babel-plugin-emotion": "^10.0.29",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "husky": "^4.2.5",
    "jest-extended": "^0.11.5",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "storybook-addon-emotion-theme": "^2.1.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.14",
    "webpack-cli": "^4.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:ci && pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 80,
    "tabWidth": 2,
    "arrowParens": "always",
    "trailingComma": "all",
    "bracketSpacing": true
  }
}
