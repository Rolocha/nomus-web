{
  "name": "nomus-server",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@typegoose/typegoose": "^6.4.0",
    "@types/zxcvbn": "^4.4.0",
    "apollo-server-express": "^2.12.0",
    "aws-sdk": "^2.669.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "graphql": "^14.6.0",
    "graphql-compose": "^7.14.1",
    "graphql-compose-mongoose": "^7.3.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "saslprep": "^1.0.3",
    "stripe": "^8.86.0",
    "type-graphql": "0.18.0-beta.17",
    "uuid-mongodb": "^2.1.3",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "build": "webpack --mode none",
    "dev": "ts-node-dev --no-deps --respawn --poll --interval 1000 --nolazy --inspect=0.0.0.0:9229 -r tsconfig-paths/register -- src",
    "serve": "node dist",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.7.10",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/qrcode": "^1.3.4",
    "@types/webpack": "^4.41.11",
    "aws-sdk-mock": "^5.1.0",
    "body-parser": "^1.19.0",
    "eslint": "^7.7.0",
    "graphql-tag": "^2.10.3",
    "husky": "^4.2.5",
    "jest": "^25.5.3",
    "node-env-run": "^3.0.2",
    "nodemon": "^2.0.3",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-modules": "^1.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && pretty-quick --staged"
    }
  },
  "proxy": "http://localhost:3001",
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 100,
    "tabWidth": 2
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js",
      "src/(.*)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.test\\.ts$",
    "testEnvironment": "node",
    "preset": "@shelf/jest-mongodb"
  }
}
